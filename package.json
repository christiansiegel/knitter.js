{
  "name": "knitter.js",
  "version": "1.0.0",
  "description": "Drawing with straight lines",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "lint": "eslint \"{*.js,src/**/*.ts}\"",
    "lint-fix": "npm run lint -- --fix",
    "pretest": "rimraf coverage",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "test-watch": "jest --watch",
    "start": "parcel src/index.html",
    "prebuild": "rimraf dist",
    "build": "parcel build --no-optimize src/index.html",
    "build-prod": "npm run build -- --no-source-maps --public-url ./"
  },
  "browserslist": [
    "since 2017-06"
  ],
  "author": "Christian Siegel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/christiansiegel/knitter.js/issues"
  },
  "homepage": "https://christiansiegel.github.io/knitter.js/",
  "devDependencies": {
    "@parcel/transformer-sass": "2.7.0",
    "@types/file-saver": "2.0.5",
    "@typescript-eslint/eslint-plugin": "5.33.1",
    "@typescript-eslint/parser": "5.33.1",
    "canvas": "2.9.3",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "28.1.3",
    "parcel": "2.7.0",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "ts-jest": "28.0.8",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "6.1.2",
    "bulma": "0.9.4",
    "bulma-slider": "2.0.5",
    "bulma-toast": "2.4.1",
    "comlink": "4.3.1",
    "file-saver": "2.0.5",
    "mobx": "6.6.1"
  },
  "resolutions": {
    "node-forge": "0.10.0"
  }
}
